{
    "ver": "0.0.1",
    "authors": [
        "bbolduc"
    ],
    "contact": "bolduc.10@osu.edu",
    "categories": ["active","virus"],
    "widgets": {
        "input": null,
        "output": "kbaseBinnedContigs"
    },
    "parameters": [ 
        {
            "id": "genomes",
            "optional": false,
            "advanced": false,
            "allow_multiple": false,
            "default_values": [ "" ],
            "field_type": "text",
            "text_options": {
                "valid_ws_types": ["KBaseGenomeAnnotations.Assembly", "KBaseGenomes.Genome"]
            }
        },
        {
            "id": "database",
            "optional": false,
            "advanced": false,
            "allow_multiple": false,
            "default_values": [ "1" ],
            "field_type": "dropdown",
            "dropdown_options": {
                "options": [{
                    "value": "1",
                    "display": "RefSeq DB",
                    "id": "RefSeq DB",
                    "ui-name": "RefSeq DB"
                },
                {
                    "value": "2",
                    "display": "Virome DB",
                    "id": "Virome DB",
                    "ui-name": "Virome DB"
                }]
            }
        },
        {
            "id": "add_genomes",
            "optional": true,
            "advanced": true,
            "allow_multiple": false,
            "default_values": [ "" ],
            "field_type": "text",
            "text_options": {
                "valid_ws_types": ["KBaseGenomeAnnotations.Assembly", "KBaseGenomes.Genome"]
            }
        },
        {
            "id": "virome",
            "optional": true,
            "advanced": true,
            "allow_multiple": false,
            "default_values": [ "1" ],
            "field_type": "checkbox",
            "checkbox_options": {
                "checked_value": "0",
                "unchecked_value": "1"
            }
        },
        {
            "id": "diamond",
            "optional": true,
            "advanced": true,
            "allow_multiple": false,
            "default_values": [ "1" ],
            "field_type": "checkbox",
            "checkbox_options": {
                "checked_value": "0",
                "unchecked_value": "1"
            }
        },
        {
            "id": "keep_db",
            "optional": true,
            "advanced": true,
            "allow_multiple": false,
            "default_values": [ "1" ],
            "field_type": "checkbox",
            "checkbox_options": {
                "checked_value": "0",
                "unchecked_value": "1"
            }
        },
        {
            "id": "no_c",
            "optional": true,
            "advanced": true,
            "allow_multiple": false,
            "default_values": [ "1" ],
            "field_type": "checkbox",
            "checkbox_options": {
                "checked_value": "0",
                "unchecked_value": "1"
            }
        },
        {
            "id": "binned_contig_name",
            "optional": false,
            "advanced": false,
            "allow_multiple": false,
            "default_values": [ "" ],
            "field_type" : "text",
            "text_options" : {
                "valid_ws_types" : [ "KBaseMetagenomes.BinnedContigs" ],
                "is_output_name" : true
            }
        }

    ],
    "behavior": {
        "service-mapping": {
            "url": "",
            "name": "VirSorter",
            "method": "run_VirSorter",
            "input_mapping": [
                {
                    "narrative_system_variable": "workspace",
                    "target_property": "workspace_name"
                },
                {
                    "input_parameter": "genomes",
                    "target_property": "genomes",
                    "target_type_transform": "resolved-ref"
                },
                {
                    "input_parameter": "database",
                    "target_property": "database"
                },
                {
                    "input_parameter": "add_genomes",
                    "target_property": "add_genomes",
                    "target_type_transform": "resolved-ref"
                },
                {
                    "input_parameter": "virome",
                    "target_property": "virome"
                },
                {
                    "input_parameter": "binned_contig_name",
                    "target_property": "binned_contig_name"
                },
                {
                    "input_parameter": "diamond",
                    "target_property": "diamond"
                },
                {
                    "input_parameter": "keep_db",
                    "target_property": "keep_db"
                },
                {
                    "input_parameter": "no_c",
                    "target_property": "no_c"
                }
            ],
            "output_mapping": [
                {
                    "service_method_output_path": [0,"report_name"],
                    "target_property": "report_name"
                },
                {
                    "service_method_output_path": [0,"report_ref"],
                    "target_property": "report_ref"
                },
                {
                    "service_method_output_path": [0,"binned_contig_obj_ref"],
                    "target_property": "binned_contig_obj_ref"
                },
                {
                    "service_method_output_path": [0,"binned_contig_obj_ref"],
                    "target_property": "objRef"
                }
            ]
        }
    },
    "job_id_output_field": "docker"
}
